{
  "ConnectionStrings": {
    "Database": "__MSSQL_DB_CONNECTION_STRING__"
  },
  "Authentication": {
    "Tokens": {
      "SecretKey": "__TOKEN_SECRET_KEY__",
      "Issuer": "BananaChips",
      "Lifetime": 1440
    },
    "PasswordPolicy": {
      "RequiredLength": 8,
      "RequiredUniqueChars": 1,
      "RequireNonAlphanumeric": true,
      "RequireLowercase": true,
      "RequireUppercase": true,
      "RequireDigit": true
    },
    "DefaultAdministrator": {
      "Email": "__PROVIDE_EMAIL_ADDRESS",
      "Password": "__PASSWORD__",
      "FirstName": "__FIRST_NAME__",
      "LastName": "__LAST_NAME__"
    }
  }
}